                var moduleid = [];
                for (i = 0; i < menu_1.length; i++) {
                    if (moduleid.findIndex(x => x._id == menu_1[i].module_id) == -1) {
                        moduleid.push({ _id: menu_1[i].module_id, _nam: menu_1[i].module_nam, _icon: menu_1[i].module_icon });
                    }
                }
                moduleid.sort((a, b) => (a._id > b._id) ? 1 : -1);
